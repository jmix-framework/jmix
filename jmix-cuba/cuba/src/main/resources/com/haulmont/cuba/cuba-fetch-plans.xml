<fetchPlans xmlns="http://jmix.io/schema/core/fetch-plans">
    <fetchPlan class="com.haulmont.cuba.security.entity.FilterEntity" name="app">
        <property name="componentId"/>
        <property name="username"/>
        <property name="name"/>
        <property name="code"/>
        <property name="xml"/>
        <property name="globalDefault"/>
    </fetchPlan>

    <fetchPlan class="com.haulmont.cuba.security.entity.SearchFolder" name="searchFolders" extends="_local">
        <property name="parent" fetchPlan="_local"/>
    </fetchPlan>

    <fetchPlan class="com.haulmont.cuba.core.entity.Config" name="appProperties" extends="_local"/>

</fetchPlans>
