<view xmlns="http://jmix.io/schema/flowui/view">
    <layout>
        <!--        Containers-->
        <article id="articleId"
                 classNames="cssClassName1, cssClassName2"
                 css="color: red;"
                 enabled="true"
                 height="50px"
                 maxHeight="55px"
                 maxWidth="120px"
                 minHeight="40px"
                 minWidth="80px"
                 themeNames="badge, primary, small"
                 title="articleTitle"
                 visible="true"
                 whiteSpace="PRE"
                 width="100px">
            <article text="articleChild"/>
        </article>

        <aside id="asideId"
               classNames="cssClassName1, cssClassName2"
               css="color: red;"
               enabled="true"
               height="50px"
               width="100px"
               maxHeight="55px"
               maxWidth="120px"
               minHeight="40px"
               minWidth="80px"
               themeNames="badge, primary, small"
               title="asideTitle"
               visible="true"
               whiteSpace="PRE">
            <aside text="asideChild"/>
        </aside>

        <descriptionList id="descriptionListId"
                         classNames="cssClassName1, cssClassName2"
                         css="color: red;"
                         enabled="true"
                         height="50px"
                         width="100px"
                         maxHeight="55px"
                         maxWidth="120px"
                         minHeight="40px"
                         minWidth="80px"
                         themeNames="badge, primary, small"
                         title="descriptionListTitle"
                         visible="true"
                         whiteSpace="PRE">
            <descriptionList text="descriptionListChild"/>
        </descriptionList>

        <term id="termId"
              classNames="cssClassName1, cssClassName2"
              css="color: red;"
              enabled="true"
              height="50px"
              maxHeight="55px"
              maxWidth="120px"
              minHeight="40px"
              minWidth="80px"
              themeNames="badge, primary, small"
              title="termTitle"
              visible="true"
              whiteSpace="PRE"
              width="100px">
            <term text="termChild"/>
        </term>

        <description id="descriptionId"
                     classNames="cssClassName1, cssClassName2"
                     css="color: red;"
                     enabled="true"
                     height="50px"
                     maxHeight="55px"
                     maxWidth="120px"
                     minHeight="40px"
                     minWidth="80px"
                     themeNames="badge, primary, small"
                     title="descriptionTitle"
                     visible="true"
                     whiteSpace="PRE"
                     width="100px">
            <description text="descriptionChild"/>
        </description>

        <div id="divId"
             classNames="cssClassName1, cssClassName2"
             css="color: red;"
             enabled="true"
             height="50px"
             maxHeight="55px"
             maxWidth="120px"
             minHeight="40px"
             minWidth="80px"
             themeNames="badge, primary, small"
             title="divTitle"
             visible="true"
             whiteSpace="PRE"
             width="100px">
            <div text="divChild"/>
        </div>

        <emphasis id="emphasisId"
                  classNames="cssClassName1, cssClassName2"
                  css="color: red;"
                  enabled="true"
                  height="50px"
                  maxHeight="55px"
                  maxWidth="120px"
                  minHeight="40px"
                  minWidth="80px"
                  themeNames="badge, primary, small"
                  title="emphasisTitle"
                  visible="true"
                  whiteSpace="PRE"
                  width="100px">
            <emphasis text="emphasisChild"/>
        </emphasis>

        <footer id="footerId"
                classNames="cssClassName1, cssClassName2"
                css="color: red;"
                enabled="true"
                height="50px"
                maxHeight="55px"
                maxWidth="120px"
                minHeight="40px"
                minWidth="80px"
                themeNames="badge, primary, small"
                title="footerTitle"
                visible="true"
                whiteSpace="PRE"
                width="100px">
            <footer text="footerChild"/>
        </footer>

        <h1 id="h1Id"
            classNames="cssClassName1, cssClassName2"
            css="color: red;"
            enabled="true"
            height="50px"
            maxHeight="55px"
            maxWidth="120px"
            minHeight="40px"
            minWidth="80px"
            themeNames="badge, primary, small"
            title="h1Title"
            visible="true"
            whiteSpace="PRE"
            width="100px">
            <h1 text="h1Child"/>
        </h1>

        <h2 id="h2Id"
            classNames="cssClassName1, cssClassName2"
            css="color: red;"
            enabled="true"
            height="50px"
            maxHeight="55px"
            maxWidth="120px"
            minHeight="40px"
            minWidth="80px"
            themeNames="badge, primary, small"
            title="h2Title"
            visible="true"
            whiteSpace="PRE"
            width="100px">
            <h2 text="h2Child"/>
        </h2>

        <h3 id="h3Id"
            classNames="cssClassName1, cssClassName2"
            css="color: red;"
            enabled="true"
            height="50px"
            maxHeight="55px"
            maxWidth="120px"
            minHeight="40px"
            minWidth="80px"
            themeNames="badge, primary, small"
            title="h3Title"
            visible="true"
            whiteSpace="PRE"
            width="100px">
            <h3 text="h3Child"/>
        </h3>

        <h4 id="h4Id"
            classNames="cssClassName1, cssClassName2"
            css="color: red;"
            enabled="true"
            height="50px"
            maxHeight="55px"
            maxWidth="120px"
            minHeight="40px"
            minWidth="80px"
            themeNames="badge, primary, small"
            title="h4Title"
            visible="true"
            whiteSpace="PRE"
            width="100px">
            <h4 text="h4Child"/>
        </h4>

        <h5 id="h5Id"
            classNames="cssClassName1, cssClassName2"
            css="color: red;"
            enabled="true"
            height="50px"
            maxHeight="55px"
            maxWidth="120px"
            minHeight="40px"
            minWidth="80px"
            themeNames="badge, primary, small"
            title="h5Title"
            visible="true"
            whiteSpace="PRE"
            width="100px">
            <h5 text="h5Child"/>
        </h5>

        <h6 id="h6Id"
            classNames="cssClassName1, cssClassName2"
            css="color: red;"
            enabled="true"
            height="50px"
            maxHeight="55px"
            maxWidth="120px"
            minHeight="40px"
            minWidth="80px"
            themeNames="badge, primary, small"
            title="h6Title"
            visible="true"
            whiteSpace="PRE"
            width="100px">
            <h6 text="h6Child"/>
        </h6>

        <header id="headerId"
                classNames="cssClassName1, cssClassName2"
                css="color: red;"
                enabled="true"
                height="50px"
                maxHeight="55px"
                maxWidth="120px"
                minHeight="40px"
                minWidth="80px"
                themeNames="badge, primary, small"
                title="headerTitle"
                visible="true"
                whiteSpace="PRE"
                width="100px">
            <header text="headerChild"/>
        </header>

        <listItem id="listItemId"
                  classNames="cssClassName1, cssClassName2"
                  css="color: red;"
                  enabled="true"
                  height="50px"
                  maxHeight="55px"
                  maxWidth="120px"
                  minHeight="40px"
                  minWidth="80px"
                  themeNames="badge, primary, small"
                  title="listItemTitle"
                  visible="true"
                  whiteSpace="PRE"
                  width="100px">
            <listItem text="listItemChild"/>
        </listItem>

        <p id="pId"
           classNames="cssClassName1, cssClassName2"
           css="color: red;"
           enabled="true"
           height="50px"
           maxHeight="55px"
           maxWidth="120px"
           minHeight="40px"
           minWidth="80px"
           themeNames="badge, primary, small"
           title="pTitle"
           visible="true"
           whiteSpace="PRE"
           width="100px">
            <p text="pChild"/>
        </p>

        <pre id="preId"
             classNames="cssClassName1, cssClassName2"
             css="color: red;"
             enabled="true"
             height="50px"
             maxHeight="55px"
             maxWidth="120px"
             minHeight="40px"
             minWidth="80px"
             themeNames="badge, primary, small"
             title="preTitle"
             visible="true"
             whiteSpace="PRE"
             width="100px">
            <pre text="preChild"/>
        </pre>

        <section id="sectionId"
                 classNames="cssClassName1, cssClassName2"
                 css="color: red;"
                 enabled="true"
                 height="50px"
                 maxHeight="55px"
                 maxWidth="120px"
                 minHeight="40px"
                 minWidth="80px"
                 themeNames="badge, primary, small"
                 title="sectionTitle"
                 visible="true"
                 whiteSpace="PRE"
                 width="100px">
            <section text="sectionChild"/>
        </section>

        <span id="spanId"
              classNames="cssClassName1, cssClassName2"
              css="color: red;"
              enabled="true"
              height="50px"
              maxHeight="55px"
              maxWidth="120px"
              minHeight="40px"
              minWidth="80px"
              themeNames="badge, primary, small"
              title="spanTitle"
              visible="true"
              whiteSpace="PRE"
              width="100px">
            <span text="spanChild"/>
        </span>

        <unorderedList id="unorderedListId"
                       classNames="cssClassName1, cssClassName2"
                       css="color: red;"
                       enabled="true"
                       height="50px"
                       maxHeight="55px"
                       maxWidth="120px"
                       minHeight="40px"
                       minWidth="80px"
                       themeNames="badge, primary, small"
                       title="unorderedListTitle"
                       visible="true"
                       whiteSpace="PRE"
                       width="100px">
            <unorderedList text="unorderedListChild"/>
        </unorderedList>

        <anchor id="anchorId"
                classNames="cssClassName1, cssClassName2"
                css="color: red;"
                enabled="true"
                height="50px"
                href="anchorHref"
                maxHeight="55px"
                maxWidth="120px"
                minHeight="40px"
                minWidth="80px"
                tabIndex="3"
                target="PARENT"
                themeNames="badge, primary, small"
                title="anchorTitle"
                visible="true"
                whiteSpace="PRE"
                width="100px">
            <anchor text="anchorChild"/>
        </anchor>

        <htmlObject id="htmlObjectId"
                    classNames="cssClassName1, cssClassName2"
                    css="color: red;"
                    data="data"
                    enabled="true"
                    height="50px"
                    maxHeight="55px"
                    maxWidth="120px"
                    minHeight="40px"
                    minWidth="80px"
                    tabIndex="3"
                    themeNames="badge, primary, small"
                    title="htmlObjectTitle"
                    type="type"
                    visible="true"
                    whiteSpace="PRE"
                    width="100px">
            <htmlObject text="htmlObjectChild"/>
        </htmlObject>

        <image id="imageId"
               alternateText="alternativeTextString"
               ariaLabel="ariaLabelString"
               classNames="cssClassName1, cssClassName2"
               css="color: red;"
               enabled="true"
               height="50px"
               maxHeight="55px"
               maxWidth="120px"
               minHeight="40px"
               minWidth="80px"
               resource="resourceString"
               themeNames="primary, small"
               title="imageTitle"
               visible="true"
               whiteSpace="PRE"
               width="100px">
            <image text="imageChild"/>
        </image>

        <main id="mainId"
              ariaLabel="ariaLabelString"
              classNames="cssClassName1, cssClassName2"
              css="color: red;"
              enabled="true"
              height="50px"
              maxHeight="55px"
              maxWidth="120px"
              minHeight="40px"
              minWidth="80px"
              themeNames="badge, primary, small"
              title="mainTitle"
              visible="true"
              whiteSpace="PRE"
              width="100px">
            <main text="mainChild"/>
        </main>

        <nav id="navId"
             ariaLabel="ariaLabelString"
             classNames="cssClassName1, cssClassName2"
             css="color: red;"
             enabled="true"
             height="50px"
             maxHeight="55px"
             maxWidth="120px"
             minHeight="40px"
             minWidth="80px"
             themeNames="badge, primary, small"
             title="navTitle"
             visible="true"
             whiteSpace="PRE"
             width="100px">
            <nav text="navChild"/>
        </nav>

        <orderedList id="orderedListId"
                     classNames="cssClassName1, cssClassName2"
                     css="color: red;"
                     enabled="true"
                     height="50px"
                     maxHeight="55px"
                     maxWidth="120px"
                     minHeight="40px"
                     minWidth="80px"
                     numberingType="UPPERCASE_LETTER"
                     themeNames="badge, primary, small"
                     title="orderedListTitle"
                     visible="true"
                     whiteSpace="PRE"
                     width="100px">
            <orderedList text="orderedListChild"/>
        </orderedList>

        <nativeLabel id="labelId"
                     classNames="cssClassName1, cssClassName2"
                     css="color: red;"
                     enabled="true"
                     height="50px"
                     maxHeight="55px"
                     maxWidth="120px"
                     minHeight="40px"
                     minWidth="80px"
                     setFor="inputId"
                     text="labelText"
                     themeNames="badge, primary, small"
                     title="labelTitle"
                     visible="true"
                     whiteSpace="PRE"
                     width="100px">
            <nativeLabel text="labelChild"/>
        </nativeLabel>

        <!--        Components-->
        <hr id="hrId"
            classNames="cssClassName1, cssClassName2"
            css="color: red;"
            height="50px"
            maxHeight="55px"
            maxWidth="120px"
            minHeight="40px"
            minWidth="80px"
            title="hrTitle"
            visible="true"
            width="100px"/>

        <iframe id="iframeId"
                allow="allowString"
                classNames="cssClassName1, cssClassName2"
                css="color: red;"
                height="50px"
                importance="HIGH"
                maxHeight="55px"
                maxWidth="120px"
                minHeight="40px"
                minWidth="80px"
                name="nameString"
                resource="resourceString"
                resourceDoc="resourceDocString"
                sandbox="ALLOW_SCRIPTS, ALLOW_TOP_NAVIGATION_BY_USER_ACTIVATION"
                title="iframeTitle"
                visible="true"
                width="100px"/>

        <input id="inputId"
               ariaLabel="ariaLabelString"
               classNames="cssClassName1, cssClassName2"
               css="color: red;"
               enabled="true"
               height="50px"
               maxHeight="55px"
               maxWidth="120px"
               minHeight="40px"
               minWidth="80px"
               placeholder="placeholderString"
               tabIndex="3"
               type="password"
               valueChangeMode="ON_CHANGE"
               valueChangeTimeout="50"
               visible="true"
               width="100px"/>

        <param id="paramId"
               classNames="cssClassName1, cssClassName2"
               css="color: red;"
               height="50px"
               maxHeight="55px"
               maxWidth="120px"
               minHeight="40px"
               minWidth="80px"
               name="nameString"
               title="paramTitle"
               value="paramValueString"
               visible="true"
               width="100px"/>

        <nativeDetails id="nativeDetailsId"
                       classNames="cssClassName1, cssClassName2"
                       css="color: red;"
                       height="50px"
                       maxHeight="55px"
                       maxWidth="120px"
                       minHeight="40px"
                       minWidth="80px"
                       open="true"
                       summaryText="detailsSummaryText"
                       title="detailsTitle"
                       visible="true"
                       width="100px">

            <span text="nativeDetailsContent"/>
        </nativeDetails>

        <rangeInput id="rangeInputId"
                    ariaLabel="ariaLabelString"
                    classNames="cssClassName1, cssClassName2"
                    css="color: red;"
                    enabled="true"
                    height="50px"
                    max="100"
                    maxHeight="55px"
                    maxWidth="120px"
                    min="10"
                    minHeight="40px"
                    minWidth="80px"
                    orientation="VERTICAL"
                    readOnly="true"
                    step="5"
                    tabIndex="3"
                    valueChangeMode="ON_CHANGE"
                    valueChangeTimeout="50"
                    visible="true"
                    width="100px"/>
    </layout>
</view>